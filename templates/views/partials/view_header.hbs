{{!-- 公用header --}}
<div id="header">
    <div class="container">
        <div role="navigation" class="navbar">
            <div class="container-fluid">
                <div class="navbar-header">
                    <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target=".navbar-collapse" aria-expanded="false">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <a href="/" class="navbar-brand"><img src="{{baseUrl "/images/common/logo@2x.png"}}"/></a>
                </div>
                <div class="collapse navbar-collapse">
                    <ul class="nav navbar-nav navbar-right nav-login">
                        <li><a href="http://cw.dudubashi.com/index.php/dudubashi/enterprise/login">企业登录</a></li>
                        <li><a href="http://cw.dudubashi.com/index.php/admin/login">&nbsp;|&nbsp;车队登录</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right nav-title">
                        {{# each navLinks}}
                            <li {{#ifeq ../section key}}class="active"{{else}}{{/ifeq}}>
                                <a href="{{ href }}">
                                    <p>{{ label }}</p>
                                    <span></span>
                                </a>
                            </li>
                        {{/each}}
                        <li class="dropdown">
                            <a class="dropdown-toggle" data-toggle="dropdown" href="#" role="button" aria-haspopup="true" aria-expanded="false">
                                关于我们 <span class="caret"></span>
                            </a>
                            <ul class="dropdown-menu">
                                {{#each navSecondLinks}}
                                    <li {{#ifeq ../section key}}class="active"{{else}}{{/ifeq}}><a href="{{ href }}">{{ label }}</a></li>
                                {{/each}}
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>